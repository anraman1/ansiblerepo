properties([parameters([string(defaultValue: '', description: 'Tower URL', name: 'turl', trim: false), string(defaultValue: '', description: 'template_name', name: 'template', trim: false)])])


timeout(unit: 'SECONDS', time: 500) {
    stage("One"){
        node {
            sleep 2
            echo "https://${params.turl}/api/v2/template_name"

    }
}
}
